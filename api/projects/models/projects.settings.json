{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "projects",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "shortTitle": {
      "type": "string",
      "required": true,
      "maxLength": 4
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "year": {
      "type": "integer",
      "required": true
    },
    "size": {
      "type": "biginteger"
    },
    "location": {
      "type": "string",
      "required": true
    },
    "icon": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "featuredMedia": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "programmatic": {
      "type": "enumeration",
      "enum": [
        "commercial",
        "urbanism",
        "publicspace",
        "culture",
        "bodyculture",
        "health",
        "education",
        "housing",
        "hotel",
        "media"
      ],
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "idea",
        "inProgress",
        "underConstruction",
        "completed"
      ]
    },
    "slug": {
      "type": "string",
      "required": true
    }
  }
}
